        "def selectionSort(l1, length):\n",
        "    \tfor i in range(length):\n",
        "\t\tmin = i\n",
        "\t\tfor j in range(i + 1, length):\n",
        "\t\t\tif l1[j] < l1[min]:\n",
        "\t\t\t\tmin = j\n",
        "\t\tl1[i], l1[min] = l1[min], l1[i]\n",
        "l1 = [2, 45, 0, 11, 9,88,97,3,7]\n",
        "length = len(l1)\n",
        "selectionSort(l1,length)\n",
        "print(\"Sorted:\",l1)"
      ]
    }
  ]
