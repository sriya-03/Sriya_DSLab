        "#Linear Search\n",
        "myList=[10,35,42,51,62]\n",
        "pivot=int(input(\"Enter a number:\"))\n",
        "for i in myList:\n",
        "  if pivot==i:\n",
        "    flag=1\n",
        "    break\n",
        "  else:\n",
        "    flag=0\n",
        "if flag==1:\n",
        "  print(\"Element Found\")\n",
        "else:\n",
        "  print(\"Element Not Found\")"
      ]
    }
  ]
